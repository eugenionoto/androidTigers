<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="myAndroid.baseball"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk android:minSdkVersion="10" />

    <application
        android:icon="@drawable/tigre"
        android:label="@string/app_name" >
        <activity
            android:name="myAndroid.baseball.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="myAndroid.baseball.ListUserActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="myAndroid.baseball.AddPlayerActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="myAndroid.baseball.CreateStatActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="myAndroid.baseball.ViewStatActivity"
            android:label="@string/app_name" >
        </activity>
    </application>

</manifest>